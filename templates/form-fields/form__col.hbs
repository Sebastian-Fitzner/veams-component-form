---
publish: false
---
<div class="form__col{{#if settings.colClasses}} {{settings.colClasses}}{{/if}}">
	{{#is settings.typeOf "input"}}
		{{> form__input }}
	{{else}}{{#is settings.typeOf "textarea"}}
		{{> form__textarea }}
	{{ else }}{{#is settings.typeOf "select"}}
		{{> form__select }}
	{{ else }}{{#is settings.typeOf "info"}}
		{{> form__info }}
	{{ else }}{{#is settings.typeOf "radio"}}
		{{> form__radio this }}
	{{ else }}{{#is settings.typeOf "checkbox"}}
		{{> form__checkbox this }}
	{{ else }}{{#is settings.typeOf "submit"}}
		{{> form__submit }}
	{{ else }}{{#is settings.typeOf "text"}}
		{{> form__text }}
	{{ else }}{{#is settings.typeOf "datepicker"}}
		{{> form__datepicker }}

	{{/is}}{{/is}}{{/is}}{{/is}}{{/is}}{{/is}}{{/is}}{{/is}}{{/is}}

</div>